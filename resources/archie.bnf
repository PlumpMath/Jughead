<ArchieLine> = Normal | Special
Special      = KeyValuePair
KeyValuePair = <WhiteSpace>* Key <WhiteSpace>* <':'> <WhiteSpace>* Value <WhiteSpace>*
Value        = (ValueWord (WhiteSpace+ ValueWord)*) WhiteSpace*
<ValueWord>  = #'[^\r\n\s\t]+'
<WhiteSpace> = #"[\t\s]"
Key          = KeyPart (<'.'> KeyPart)*
<KeyPart>    = #'[\w\-]+'
Normal       = AnyValue
AnyValue     = #".*"
